<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>fr.mathieudarse.ipv6tun.NetworkChange</string>
	<key>LowPriorityIO</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/sbin/ipv6tun</string>
		<string>-c</string>
		<string>/usr/local/etc/ipv6tun.conf</string>
		<string>network-change</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/iptvtun.err.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/iptvtun.out.log</string>
	<key>WatchPaths</key>
	<array>
		<string>/etc/resolv.conf</string>
		<string>/Library/Preferences/SystemConfiguration/NetworkInterfaces.plist</string>
		<string>/Library/Preferences/SystemConfiguration/com.apple.airport.preferences.plist</string>
	</array>
</dict>
</plist>
